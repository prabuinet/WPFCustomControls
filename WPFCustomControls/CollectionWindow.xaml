<Window x:Class="WPFCustomControls.CollectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFCustomControls"
        xmlns:cc1="clr-namespace:WpfCustomControlLibrary1;assembly=WpfCustomControlLibrary1"
        mc:Ignorable="d"
        Title="CollectionWindow" Height="450" Width="800">
    <Grid>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <cc1:MyCollectionControl x:Name="list1" Grid.Column="0"></cc1:MyCollectionControl>
            <cc1:MyCollectionControl x:Name="list2" Grid.Column="1"></cc1:MyCollectionControl>
        </Grid>
    </Grid>    
</Window>
